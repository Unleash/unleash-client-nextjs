{
  "name": "@unleash/nextjs",
  "version": "1.4.3",
  "description": "Unleash SDK for Next.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "bin": {
    "unleash": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "default": "./client.js"
    }
  },
  "files": [
    "dist",
    "client.*",
    "src"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts* client.ts",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "build": "tsc && tsc --p tsconfig.client.json && cp ../README.md ./README.md",
    "dev": "tsc -w"
  },
  "devDependencies": {
    "@types/murmurhash3js": "3.0.7",
    "@types/node": "20.11.30",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/semver": "7.5.8",
    "@unleash/client-specification": "5.1.6",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint-config-custom": "workspace:*",
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tsconfig": "workspace:*",
    "typescript": "5.4.5",
    "vite": "5.2.13",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "@commander-js/extra-typings": "12.1.0",
    "@next/env": "14.2.4",
    "@unleash/proxy-client-react": "4.2.4",
    "commander": "12.1.0",
    "murmurhash3js": "3.0.1",
    "semver": "7.6.2",
    "unleash-client": "5.5.3",
    "unleash-proxy-client": "3.4.1"
  },
  "peerDependencies": {
    "next": ">=12",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Unleash/unleash-client-nextjs"
  },
  "bugs": {
    "url": "https://github.com/Unleash/unleash-client-nextjs/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
