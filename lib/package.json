{
  "name": "@unleash/nextjs",
  "version": "0.1.3-alpha.2",
  "description": "Unleash SDK for Next.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "bin": {
    "unleash": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts*",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "build": "tsc",
    "dev": "tsc -w",
    "prepack": "cp ../README.md ./README.md"
  },
  "devDependencies": {
    "@types/ip": "1.1.0",
    "@types/murmurhash3js": "3.0.3",
    "@types/node": "18.14.5",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/semver": "7.5.0",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint-config-custom": "workspace:*",
    "next": "13.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsconfig": "workspace:*",
    "typescript": "5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^10.0.3",
    "@next/env": "^13.4.1",
    "@unleash/proxy-client-react": "3.5.2",
    "commander": "^10.0.1",
    "ip": "1.1.8",
    "murmurhash3js": "3.0.1",
    "semver": "7.5.0",
    "unleash-client": "3.19.1",
    "unleash-proxy-client": "2.4.3"
  },
  "peerDependencies": {
    "next": ">=12",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Unleash/unleash-client-nextjs"
  },
  "bugs": {
    "url": "https://github.com/Unleash/unleash-client-nextjs/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
