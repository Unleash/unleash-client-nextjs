{
  "name": "@unleash/nextjs",
  "version": "1.0.0",
  "description": "Unleash SDK for Next.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "bin": {
    "unleash": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./client.d.ts",
      "default": "./client.js"
    }
  },
  "files": [
    "dist",
    "client.*",
    "src"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts* client.ts",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "build": "tsc && tsc --p tsconfig.client.json && cp ../README.md ./README.md",
    "dev": "tsc -w",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@types/murmurhash3js": "3.0.3",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/semver": "7.5.0",
    "@unleash/client-specification": "4.2.2",
    "@vitest/coverage-c8": "^0.32.2",
    "@vitest/coverage-v8": "^0.32.2",
    "eslint-config-custom": "workspace:*",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsconfig": "workspace:*",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vitest": "0.32.2"
  },
  "dependencies": {
    "@commander-js/extra-typings": "11.0.0",
    "@next/env": "13.4.7",
    "@unleash/proxy-client-react": "3.6.0",
    "commander": "11.0.0",
    "murmurhash3js": "3.0.1",
    "semver": "7.5.3",
    "unleash-client": "4.0.1",
    "unleash-proxy-client": "2.5.0"
  },
  "peerDependencies": {
    "next": ">=12",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Unleash/unleash-client-nextjs"
  },
  "bugs": {
    "url": "https://github.com/Unleash/unleash-client-nextjs/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
